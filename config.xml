<?xml version="1.0" encoding="utf-8"?><widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.ms.app" version="1.0.0">
    <name>MS 1.0</name>
    <description>MS First App</description>
    <author email="omar11ms@gmail.com" href="omar11ms@gmail.com">Omar Saleh</author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
      <icon src="www/assets/icn/Icon-36.png" density="ldpi"/>
      <icon src="www/assets/icn/Icon-48.png" density="mdpi"/>
      <icon src="www/assets/icn/Icon-72.png" density="hdpi"/>
      <icon src="www/assets/icn/Icon-96.png" density="xhdpi"/>
      <icon src="www/assets/icn/Icon-144.png" density="xxhdpi"/>
      <icon src="www/assets/icn/Icon-192.png" density="xxxhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>